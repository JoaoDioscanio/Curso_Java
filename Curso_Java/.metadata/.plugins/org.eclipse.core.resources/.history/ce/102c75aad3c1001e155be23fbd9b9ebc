package exercicios;

import java.util.Scanner;
public class Ex_011 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Escolha um item da tabela:");
		int tag = sc.nextInt();
		while (tag < 1 || tag > 5) {
			System.out.println("Código inválido, digite novamente:");
			tag = sc.nextInt();
		}
	
		sc.close();
	}

}
