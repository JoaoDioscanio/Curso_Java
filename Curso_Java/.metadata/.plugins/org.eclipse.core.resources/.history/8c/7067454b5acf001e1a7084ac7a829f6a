package aulas;

import java.util.Scanner;

public class Matrizes {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int[][] matriz = new int[n][3];
		for (int i = 0; i < n; i++) {
			int v = sc.nextInt();
			v = matriz[i][0];
		}
		sc.close();
	}

}
